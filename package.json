{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:full": "node scripts/dev.js --with-queue",
        "dev:server": "node scripts/dev.js",
        "dev:concurrent": "concurrently -c \"#93c5fd,#fb7185\" \"npm run serve\" \"npm run dev\" --names=\"server,vite\" --kill-others-on-fail",
        "serve": "php artisan serve",
        "queue": "php artisan queue:listen --tries=1",
        "logs": "php artisan log:tail",
        "watch": "vite --watch"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "autoprefixer": "^10.4.21",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.13",
        "vite": "^7.0.4"
    }
}
